<h1>Student Form</h2>
<%= form_tag students_path do %>
    First Name: <input type="text" id="first_name" name="student[first_name]"><br>
    Last Name: <input type="text" id="last_name" name="student[last_name]"><br>
    <%= hidden_field_tag :authenticity_token, :form_authenticity_token %>
    <input type="submit" value="Submit Student">
<% end %>